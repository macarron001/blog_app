Articles
<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Content</th>
    </tr>

  </thead>

  <tbody>
    <% @articles.each do |article| %>
      <tr>
        <td><%= article.title %> </td>
        <td><%= article.body %> </td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= link_to 'New Article', new_article_path %>